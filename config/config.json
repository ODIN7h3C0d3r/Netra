{
  "api": {
    "base_url": "https://ipapi.co ",
    "token": "", 
    "retry_limit": 3,
    "timeout": "10s"
  },
  "cache": {
    "enabled": true,
    "ttl": "24h"
  },
  "format": {
    "default": "text",
    "fields": "ip,country,region,city,isp,asn,is_hosting"
  },
  "network": {
    "proxy": "",
    "dns_servers": ["8.8.8.8", "1.1.1.1"]
  },
  "ui": {
    "color_theme": "dark",
    "quiet_mode": false
  }
}